<% layout("layout/Boilerplate") %>
    <h1>All Campgrounds</h1>
    <a href="/campgrounds/new">Create a New Campground</a>
    <ul>
        <% for (let campground of campgrounds) { %>
            <li> <a href="/campgrounds/<%= campground.id %>">
                    <%= campground.title %>
                </a> </li>
            <% } %>
    </ul>